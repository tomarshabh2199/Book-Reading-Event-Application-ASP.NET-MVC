
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index Page</title>
    @RenderSection("headsection",required:false)
</head>
<body>
    <div class="jumbotron"> 
        <table border="8" width="1000" align="center" cellspacing="20" cellpadding="20">
            <tr style="background-color:darkkhaki; color:white; text-align:center">
            <h1><td colspan="2">Event Page</td></h1>
            </tr>
            <tr height="100">
                <td width="200">
                    <ul>
                     <h2><li>@Html.ActionLink("GotoEvent", "GotoEvent")</li></h2>
                      <h2><li>@Html.ActionLink("EventLink", "EventLink")</li></h2>
                        <h2><li>@Html.ActionLink("EventEdit", "EventEdit")</li></h2>
                    </ul>
                </td>
                <td width="550">
                    @RenderBody()
                </td>
            </tr>
            <tr style="background-color:darkseagreen; color:white; text-align:center">
                <td>
                    &copy; copyright 2021<br>
                    Events| Invited Events | Go to Event Page
                </td>
            </tr>
        </table>
    </div>
</body>
</html>
